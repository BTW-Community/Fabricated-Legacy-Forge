  "license": "CC0-1.0",
  "icon": "assets/fabricated-forge/icon.png",
  "environment": "*",
  "accessWidener": "fabricated-forge.accesswidener",
  "entrypoints": {
    "preLaunch": [
      "fr.catcore.fabricatedforge.FabricatedForge"
